{
    "$schema": "./node_modules/oxfmt/configuration_schema.json",
    "useTabs": true,
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "ignorePatterns": [
        "node_modules",
        ".svelte-kit",
        "build",
        "dist",
        "**/*.svelte",
        "scripts"
    ]
}